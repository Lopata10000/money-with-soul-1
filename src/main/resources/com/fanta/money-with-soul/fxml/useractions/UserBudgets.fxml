<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXButton?>
<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.fanta.moneywithsoul.controller.useractions.UserBudgetsNodeController"
      styleClass="rounded-stackpane-main" spacing="10">
    <Label fx:id="budgetNameLabel" styleClass="label-info" style="-fx-padding: 0 20 0 20;"/>
    <Label fx:id="budgetAmountLabel" styleClass="label-info"/>
    <Label fx:id="budgetStartDateLabel" styleClass="label-info"/>
    <Label fx:id="budgetEndDateLabel" styleClass="label-info"/>
    <StackPane>
        <JFXButton fx:id="deleteBudgetButton" onAction="#deleteBudget" mnemonicParsing="false" text="Delete" styleClass="jfx-button-item" />
    </StackPane>
</VBox>
